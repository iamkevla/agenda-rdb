{
  "name": "agenda-rdb",
  "version": "0.0.10",
  "description": "Light weight job scheduler for Node.js",
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "config": {
    "blanket": {
      "pattern": "lib/*",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/iamkevla/agenda-rdb.git"
  },
  "keywords": [
    "job",
    "jobs",
    "cron",
    "delayed",
    "scheduler",
    "runner"
  ],
  "author": "Kevin Vlahos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamkevla/agenda-rdb/issues"
  },
  "dependencies": {
    "cron": "~1.1.0",
    "date.js": "~0.2.0",
    "human-interval": "~0.1.3",
    "moment-timezone": "^0.5.0",
    "rethinkdbdash": "^2.2.12"
  },
  "devDependencies": {
    "blanket": "1.2.2",
    "coveralls": "~2.11.4",
    "expect.js": "~0.3.1",
    "mocha": "~2.4.5",
    "mocha-lcov-reporter": "1.2.0",
    "q": "~1.4.1"
  }
}
